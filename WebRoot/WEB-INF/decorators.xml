<?xml version="1.0" encoding="ISO-8859-1"?>

<decorators defaultdir="/decorators">
    <!-- Any urls that are excluded will never be decorated by Sitemesh -->
    <excludes>
        <pattern>/exclude/*</pattern>
		<pattern>/encodeTest.htm</pattern>	
		<pattern>encodeTest.htm</pattern>	
    </excludes>

	<decorator name="main" page="main.jsp">
        <pattern>/*.htm</pattern>
    </decorator>   
	
	<decorator name="ontoclassify" page="ontoclassify.jsp">
		<pattern>/*.cla</pattern>
	</decorator>
	
	<decorator name="search" page="search.jsp">
		<pattern>/*.luc</pattern>
	</decorator>	
	
	<decorator name="blank" page="blank.jsp"/>
	
	<decorator name="navigator" page="navigator.jsp"/>
</decorators>