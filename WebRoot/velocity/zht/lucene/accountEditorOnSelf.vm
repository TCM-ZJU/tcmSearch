<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
"http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=GBK">
<title>帳戶信息[OnSelf]</title>
<link href="css/register.css" rel="stylesheet" type="text/css">

</HEAD>

<BODY>


<table cellspacing=1 cellpadding=3 align=center class="tableBorder2" width="80%" >
<tr><td height=25 valign=middle>
<img src="${rc.contextPath}/images/dws.gif" align=absmiddle width=48>→ 
帳戶信息</a>
</td></td>
</table>

#if(${accEdit.invisable})
<div align="center">對不起，您無權查看此信息</div>
#else
<!--------------------------- large else part ---------------------------//-->
<form name="AccountEditorForm" action="secure/accountEditor.luc" method="post">

<table width=80% cellpadding=3 cellspacing=1 align=center class="tableBorder1" border="1">
<TBODY> 
<TR align=middle> 
<Th colSpan=2 height=24>帳戶信息
  <!--${accountPre.isToRunOutFee()}//-->
  #if(${accountPre.isToRunOutFee()})
   &nbsp<font color=#ff0000>[帳戶余額不足]</font>
  #end
</Th>
</TR>

<TR> 
<TD width=40% class="TableBody1"><B>姓名</B>:</font></TD>
<TD width=60%  class="TableBody1"> 
#springBind("account.userInfo.contact_name")
<INPUT maxLength="10" size=30 name="${status.expression}" value="${accountPre.userInfo.contact_name}" #if($accEdit.userInfo) DISABLED #end>&nbsp;&nbsp</td>
</TR>

<TR> 
<TD width=40%  class="TableBody1"><B>性別</B>:</TD>
<TD width=60%  class="TableBody1"> 
#springBind("account.userInfo.sex")
<INPUT type=radio #if(${accountPre.userInfo.sex}=="1")CHECKED #end value=1 name="${status.expression}"  #if($accEdit.userInfo) DISABLED #end>男 &nbsp;&nbsp;&nbsp;&nbsp; 
<INPUT type=radio #if(${accountPre.userInfo.sex}=="0")CHECKED #end value=0 name="${status.expression}"  #if($accEdit.userInfo) DISABLED #end>女</font></TD>
</TR>

<TR> 
<TD width=40% class="TableBody1"><B>聯系方式</B>:</TD>
<TD width=60%  class="TableBody1"> 
#springBind("account.userInfo.contact")
<INPUT maxLength="30" size=30 name="${status.expression}" value="${accountPre.userInfo.contact}"  #if($accEdit.userInfo) DISABLED #end>
</TD>
</TR>


<TR> 
<TD width=40%  class="TableBody1"><B>登錄方式</B>:</TD>
<TD width=60%  class="TableBody1">
#springBind("account.loginType")
<INPUT type=radio value="LOGIN_TYPE_USERPASSWORD" name="${status.expression}" #if(${accountPre.loginType}=="LOGIN_TYPE_USERPASSWORD") CHECKED #end  #if($accEdit.loginType) DISABLED #end>帳號 &nbsp;&nbsp;&nbsp;&nbsp; 
#springBind("account.loginType")
<INPUT type=radio value="LOGIN_TYPE_IP" name="${status.expression}" #if(${accountPre.loginType}=="LOGIN_TYPE_IP") CHECKED #end  #if($accEdit.loginType) DISABLED #end>IP登陸</font></TD>
</TR>


<TR> 
<TD width=40%  class="TableBody1"><B>計費方式</B>:</TD>
<TD width=60%  class="TableBody1"> 
 #springBind("account.chargeType")
<INPUT type=radio value="CHARGE_TYPE_ONMONTH" name="${status.expression}" #if(${accountPre.chargeType}=="CHARGE_TYPE_ONMONTH") CHECKED #end  #if($accEdit.chargeType) DISABLED #end>包月 &nbsp;&nbsp;&nbsp;&nbsp; 
<INPUT type=radio value="CHARGE_TYPE_ONFLOW" name="${status.expression}" #if(${accountPre.chargeType}=="CHARGE_TYPE_ONFLOW") CHECKED #end #if($accEdit.chargeType) DISABLED #end>流量</TD>
</TR>

<TR> 
<TD width=40% class="TableBody1"><B>用戶名</B>:</TD>
<TD width=60%  class="TableBody1"> 
#springBind("account.username")
<INPUT maxLength="10" size=30 name="${status.expression}" value="${accountPre.username}" #if($accEdit.username) DISABLED #end/>
</TD>
</TR>

<TR> 
<TD width=40% class="TableBody1"><B>密碼:</TD>
<TD class="TableBody1"> 
#springBind("account.password")
<INPUT type=text maxLength=16 size=30 name="${status.expression}" value="${accountPre.password}" #if($accEdit.password) DISABLED #end/>
</TD>
</TR>

<TR> 
<TD width=40% class="TableBody1"><B>IP地址</B>:</TD>
<TD width=60%  class="TableBody1"> 
#springBind("account.IPAddress")
<INPUT maxLength="30" size=30 name="${status.expression}" value="${accountPre.IPAddress}" #if($accEdit.IPAddress) DISABLED #end/>
</TD>
</TR>

<TD width=40% class="TableBody1"><B>子網掩碼</B>:</TD>
<TD width=60%  class="TableBody1"> 
#springBind("account.mask")
<INPUT maxLength="30" size=30 name="${status.expression}" value="${accountPre.mask}" #if($accEdit.mask) DISABLED #end/>
</TD>
</TR>


<TR> 
<TD width=40% class="TableBody1"><B>總的月數</B>︰</TD>
<TD class="TableBody1"> 
 #springBind("account.totalDate")
<INPUT maxLength="10" size=30 name="${status.expression}" value="${accountPre.totalDate}" #if($accEdit.totalDate) DISABLED #end>
</TD>
</TR>

<TR> 
<TD width=40% class="TableBody1"><B>開始時間</B>︰</TD>
<TD class="TableBody1"> 
#springBind("account.startDate")
<INPUT maxLength="10" size=30 name="${status.expression}" value="${accountPre.startDate}" #if($accEdit.startDate) DISABLED #end>
</TD>
</TR>

<TR> 
<TD width=40% class="TableBody1"><B>總的流量</B>︰</TD>
<TD class="TableBody1"> 
#springBind("account.totalFlow")
<INPUT maxLength="10" size=30 name="${status.expression}" value="${accountPre.totalFlow}" #if($accEdit.totalFlow) DISABLED #end>
</TD>
</TR>

<TR> 
<TD width=40% class="TableBody1"><B>已使用流量</B>︰</TD>
<TD class="TableBody1"> 
#springBind("account.usedFlow")
<INPUT maxLength="10" size=30 name="${status.expression}" value="${accountPre.usedFlow}" #if($accEdit.usedFlow) DISABLED #end>
</TD>
</TR>

<TR> 
<TD width=40% class="TableBody1"><B>最多在線用戶數</B>︰</TD>
<TD class="TableBody1"> 
#springBind("account.maxConcurrence")
<INPUT maxLength="10" size=30 name="${status.expression}" value="${accountPre.maxConcurrence}" #if($accEdit.maxConcurrence) DISABLED #end>
</TD>
</TR>

<TR> 
<TD width=40% class="TableBody1"><B>消費記錄</B>︰</TD>
<TD class="TableBody1"> 
<a href = "$rc.contextPath/secure/accessRecord.luc?accountId=${accountPre.id}&onself=yes">查看詳細
</TD>
</TR>

<TR> 
<TD width=40% class="TableBody1"><B>訂購本體</B>︰</TD>
<TD class="TableBody1"> 
<a href = "$rc.contextPath/secure/ontologyAccessPrivilegeSignmentOnUser.luc">查看詳細
</TD>
</TR>

<TR> 
<TD width=40% class="TableBody1"><B>訂購數據庫</B>︰</TD>
<TD class="TableBody1"> 
<a href = "$rc.contextPath/secure/tableAccessPrivilegeSignmentOnUser.luc">查看詳細
</TD>
</TR>

<TR> 
<TD width=40%  class="TableBody1"><B>帳戶狀態</B>:</TD>
<TD width=60%  class="TableBody1"> 
 #springBind("account.status")
<INPUT type=radio value="ACCOUNT_STATUS_NORAML" name="${status.expression}" #if(${accountPre.status}=="ACCOUNT_STATUS_NORAML") CHECKED #end #if($accEdit.status) DISABLED #end>正常 &nbsp;&nbsp;&nbsp;&nbsp; 
<INPUT type=radio value="ACCOUNT_STATUS_STOPPED" name="${status.expression}" #if(${accountPre.status}=="ACCOUNT_STATUS_STOPPED") CHECKED #end #if($accEdit.status) DISABLED #end>暫停 &nbsp;&nbsp;&nbsp;&nbsp; 
<INPUT type=radio value="ACCOUNT_STATUS_INACTIVATED" name="${status.expression}" #if(${accountPre.status}=="ACCOUNT_STATUS_INACTIVATED") CHECKED #end #if($accEdit.status) DISABLED #end>未激活</TD>
</TR>
</TBODY>
</table>

<table cellpadding=0 cellspacing=0 border=0 width=97% align=center>
<tr></tr>
<tr></tr>
<tr></tr>
<tr></tr>
<tr align=center>
<td width=50% ><input type=button value="修 改" onclick="doSubmit(true)">&nbsp;&nbsp<input type=button value="返 回" onclick="doSubmit(false)"></td>
</tr>
</table>

</form>
#end
<!--------------------------- large else part (end here) ---------------------------  //-->
<script>
function doSubmit(update)
{
 if(update)
 {
   document.AccountEditorForm.action = "${rc.contextPath}/secure/accountEditorOnSelf.luc?AccountId=${accountPre.id}";
   document.AccountEditorForm.submit();
 }else{
   document.AccountEditorForm.action = "${rc.contextPath}/index.jsp";
   document.AccountEditorForm.submit();
 }   
}
</script>
